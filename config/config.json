{
  "listeners": {
    "*:80": {
      "pass": "routes"
    }
  },
  "routes": [
    {
      "match": {
        "uri": "/static/*"
      },
      "action": {
        "share": "/build$uri"
      }
    },
    {
      "match": {
        "uri": "/media/*"
      },
      "action": {
        "share": "/build$uri"
      }
    },
    {
      "action": {
        "pass": "applications/django"
      }
    }
  ],
  "applications": {
    "django": {
      "type": "python 3.10",
      "user": "root",
      "home": "/build/venv/",
      "processes": 3,
      "threads": 1,
      "path": "/build/",
      "module": "tribal_wars_planer.wsgi",
      "limits": {
        "timeout": 90,
        "requests": 1000
      }
    }
  },
  "access_log": "/var/log/access.log"
}
