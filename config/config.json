{
    "listeners": {
        "*:80": {
            "pass": "routes"
        }
    },
    "routes": [
        {
            "match": {
                "uri": "/static/*"
            },
            "action": {
                "share": "/build"
            }
        },
        {
            "match": {
                "uri": "/media/*"
            },
            "action": {
                "share": "/build"
            }
        },
        {
            "action": {
                "pass": "applications/django"
            }
        }
    ],
    "applications": {
        "django": {
            "type": "python 3.9",
            "path": "/build/",
            "module": "tribal_wars_planer.wsgi"
        }
    },
    "access_log": "/var/log/access.log"
}